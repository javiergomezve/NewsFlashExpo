{
  "name": "newsflashexpo",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint:fix": "yarn lint --fix",
    "generate:gql:schema": "yarn graphql-codegen --config ./codegen.schema.yml",
